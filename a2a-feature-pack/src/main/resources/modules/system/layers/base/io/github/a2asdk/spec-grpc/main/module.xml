<module xmlns="urn:jboss:module:1.9" name="io.github.a2asdk.spec-grpc">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${io.github.a2asdk:a2a-java-sdk-spec-grpc}"/>
    </resources>

    <dependencies>
        <module name="io.github.a2asdk.spec"/>
        <module name="io.github.a2asdk.jsonrpc-common"/>
        <module name="io.grpc-all" optional="true"/>
        <module name="com.google.protobuf"/>
        <module name="com.google.protobuf.util"/>
        <module name="com.google.api.grpc.proto-google-common-protos"/>
        <module name="java.logging"/>
        <module name="com.google.code.gson"/>
    </dependencies>
</module>
