<module xmlns="urn:jboss:module:1.9" name="io.github.a2asdk.client">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <artifact name="${io.github.a2asdk:a2a-java-sdk-client}"/>
    </resources>

    <dependencies>
        <module name="io.github.a2asdk.common"/>
        <module name="io.github.a2asdk.spec"/>
        <module name="io.github.a2asdk.http-client"/>
        <module name="io.github.a2asdk.client-transport-spi"/>
        <module name="io.github.a2asdk.client-transport-jsonrpc"/>
        <module name="io.github.a2asdk.client-transport-grpc" optional="true"/>
        <module name="io.github.a2asdk.client-transport-rest" optional="true"/>
        <module name="java.logging"/>
    </dependencies>
</module>
